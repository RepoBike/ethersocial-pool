{{#if model.matured}}
<h4>확정 블록</h4>
<div class="table-responsive">
  <table class="table table-condensed table-striped">
    <thead>
      <tr>
        <th>블록번호</th>
        <th>블록Hash</th>
        <th>발견시간</th>
        <th>작업비율</th>
        <th>보상</th>
      </tr>
    </thead>
    <tbody>
      {{#each model.matured as |block|}}
        {{partial "blocks/block"}}
      {{/each}}
    </tbody>
  </table>
</div>
{{else}}
<h3>발견된 블록 없음</h3>
{{/if}}
